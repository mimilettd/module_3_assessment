<h1><%= @stores.count %> Total Stores</h1>

<% @stores.each do |store| %>
  <div class="store">
    <p class="longname"><%= store.longname %></p>
    <p class="city"><%= store.city %></p>
    <p class="distance"><%= store.distance %></p>
    <p class="phone"><%= store.phone %></p>
    <p class="type"><%= store.type %></p>
  </div>
<% end %>
